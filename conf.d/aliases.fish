# git log
alias gl 'git log --graph --pretty=format:"%C(dim blue)%h%C(reset) - %C(bold blue)(%ar) %C(nobold white)%s%C(reset) %C(bold white)[by %aN]%C(auto)%d%C(reset)"'
alias gla 'git log --graph --all --pretty=format:"%C(dim blue)%h%C(reset) - %C(bold blue)(%ar) %C(nobold white)%s%C(reset) %C(bold white)[by %aN]%C(auto)%d%C(reset)"'
